<link rel="stylesheet" href="http://dhbhdrzi4tiry.cloudfront.net/cdn/sites/foundation.min.css">

<div class="off-canvas-wrapper">
  <div class="callout primary">
    <div class="row column">
      <a href="/" class="gohome"><div class="header title">3 Museos</div></a>
      <div class="header action"><%= link_to "Salir", destroy_admin_session_path, :method => :delete %></div>
    </div>
  </div>
  <br>
  <div class="row">
    <table id="datatable">
        <thead>
            <tr>
                <th>Usuario</th>
                <th>Puntaje</th>
                <th>Intentos</th>
            </tr>
        </thead>

        <tbody>
            <% @trypoints.each do |trypoint| %>
            <tr>
                <td><%= trypoint.user.email %></td>
                <td><%= trypoint.points %></td>
                <td><%= trypoint.numTry %></td>
            </tr>
            <% end %>
        </tbody>
    </table>
  </div>

</div>
